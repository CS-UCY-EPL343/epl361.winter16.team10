<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.irini.cafeorderingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Home"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Menu" />

        <intent-filter>
            <action android:name="com.example.irini.cafeorderingsystem.Menu" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".ContactUs" />

        <intent-filter>
            <action android:name="com.example.irini.cafeorderingsystem.Order" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".Order">
            <intent-filter>
                <action android:name="com.example.irini.cafeorderingsystem.Order" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".FAQs" />
        <activity android:name=".Next" />
        <activity android:name=".snacks"></activity>
    </application>

</manifest>